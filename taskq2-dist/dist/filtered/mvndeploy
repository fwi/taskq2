mvn deploy:deploy-file -Dpackaging=pom -DpomFile=${dist.artifactId}-parent-${project.version}-pom.xml -Dfile=${dist.artifactId}-parent-${project.version}-pom.xml -Durl=file:///maven-repo/releases
mvn deploy:deploy-file -Dfile=${dist.artifactId}-core-${project.version}.jar -DpomFile=${dist.artifactId}-core-${project.version}-pom.xml -Dsources=${dist.artifactId}-core-${project.version}-sources.jar -Durl=file:///maven-repo/releases
mvn deploy:deploy-file -Dfile=${dist.artifactId}-jdbc-${project.version}.jar -DpomFile=${dist.artifactId}-jdbc-${project.version}-pom.xml -Dsources=${dist.artifactId}-jdbc-${project.version}-sources.jar -Durl=file:///maven-repo/releases
